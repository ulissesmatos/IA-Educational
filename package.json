{
  "name": "ia-ou-nao",
  "version": "1.0.0",
  "type": "module",
  "description": "Ferramenta gamificada para oficina IA na Educação",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev:docker": "docker compose up --build",
    "dev:docker:down": "docker compose down",
    "dev:db": "docker run --name postgres-dev -e POSTGRES_PASSWORD=postgres -e POSTGRES_DB=iaounao -p 5432:5432 -d postgres:16-alpine",
    "dev:db:stop": "docker stop postgres-dev && docker rm postgres-dev",
    "build": "tsc",
    "start": "node dist/server.js",
    "db:migrate": "prisma migrate deploy",
    "db:seed": "prisma db seed",
    "db:generate": "prisma generate",
    "db:reset": "prisma migrate reset --force",
    "db:init": "tsx scripts/init-db.ts",
    "db:check": "tsx scripts/check-db.ts",
    "db:status": "tsx scripts/status-db.ts",
    "admin:setup": "tsx scripts/setup-admin.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "keywords": [
    "quiz",
    "education",
    "ai",
    "gamification"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.10.0",
    "@types/bcrypt": "^6.0.0",
    "@types/express-session": "^1.18.2",
    "@types/pg": "^8.16.0",
    "bcrypt": "^6.0.0",
    "connect-pg-simple": "^10.0.0",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.19.0",
    "express-validator": "^7.3.1",
    "helmet": "^8.1.0",
    "method-override": "^3.0.0",
    "multer": "^2.0.2",
    "pg": "^8.17.2",
    "socket.io": "^4.7.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/connect-pg-simple": "^7.0.3",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "@types/method-override": "^3.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.11.0",
    "prisma": "^5.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
